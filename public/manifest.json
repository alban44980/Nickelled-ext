{
  "manifest_version": 2,
  "name": "Nickelled-ext",
  "author": "Alban",
  "version": "1.0.1",
  "description": "Allows to login with your Google Account !",
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "identity"]
}
